{
  "name": "naytak-myanmar-react-calendar",
  "version": "0.1.0",
  "description": "React Calendar Component using Naytak Calendar Core",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts",
    "dev": "tsup src/index.tsx --watch --format esm,cjs --dts",
    "start": "vite"
  },
  "dependencies": {
    "naytak-calendar-core": "0.1.0",
    "@vitejs/plugin-react": "^5.1.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0"
  }
}
